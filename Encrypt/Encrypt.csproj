<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>10.0</LangVersion>
    <PackageId>Encrypt.String</PackageId>
    <Version>1.0.1</Version>
    <License>MIT</License>
    <Authors>Alexandre Bartie</Authors>
    <Company>bartie.devops</Company>
    <PackageDescription>Extension for Encrypt String</PackageDescription>
    <Description>A string extension method that returns its encrypted value.</Description>
    <RepositoryUrl>https://github.com/AlexandreBartie/Encrypt</RepositoryUrl>
    <PackageProjectUrl>https://github.com/AlexandreBartie/Encrypt</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>Encrypt</PackageTags>
    <Title>Encrypt Extensions</Title>
    <Summary>Extension for Encrypt String</Summary>
    <PackageIcon>encrypt.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="encrypt.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="encrypt.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
